{
	"name": "@wholly-trinity/web",
	"scripts": {
		"dev": "solid-start dev -p $WEB_PORT",
		"build": "npm run build:solid && npm run build:solid-router && npm run build:project && npm run build:deployment",
		"build:deployment": "echo ls -al public && ls -al public && echo ls -al .vercel/output/static && ls -al .vercel/output/static",
		"build:project": "solid-start build",
		"build:solid": "pushd ../../vendor/solid/packages/solid && pnpm run build types link && popd",
		"build:solid-router": "pushd ../../vendor/solid-router && pnpm run build && popd",
		"start": "solid-start start"
	},
	"type": "module",
	"devDependencies": {
		"@ctx-core/html": "^11.0.50",
		"@ctx-core/object": "^22.2.7",
		"@ctx-core/ui-solid": "^2.0.13",
		"@rollup/plugin-replace": "^4.0.0",
		"@solidjs/meta": "^0.28.0",
		"@solidjs/router": "^0.4.3",
		"@wholly-trinity/api": "workspace:*",
		"@wholly-trinity/types": "workspace:*",
		"@wholly-trinity/ui": "workspace:*",
		"@wholly-trinity/validation": "workspace:*",
		"autoprefixer": "^10.4.8",
		"itty-router": "^2.6.1",
		"postcss": "^8.4.16",
		"solid-heroicons": "^3.0.1",
		"solid-js": "^1.5.4",
		"solid-meta": "^0.28.1",
		"solid-start": "next",
		"solid-start-node": "next",
		"solid-start-vercel": "next",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.8.2",
		"undici": "^5.10.0",
		"vite": "^3.0.9"
	},
	"engines": {
		"node": ">=14"
	}
}